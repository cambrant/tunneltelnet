#!/usr/bin/env python

import sys
import datetime

outputfile = sys.argv[1]
of = open(outputfile, "w")

while True:
	input_data = sys.stdin.readline()
	if input_data:
		timestamp = datetime.datetime.now().strftime('%Y-%m-%d %H:%M:%S: ')
		of.write(timestamp + input_data)
		sys.stdout.write(input_data)
	else:
		break

of.close()
